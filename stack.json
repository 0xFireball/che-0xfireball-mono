{
  "name": "0xfireball-mono",
  "description": "0xFireball's mono/c# Stack",
  "scope": "general",
  "tags": [
    "mono",
    "x11",
    "blackbox"
  ],
  "components": [
    {
      "name": "Mono",
      "version": "latest"
    },
    {
      "name": "x11",
      "version": "---"
    }
  ],
  "source": {
    "type": "image",
    "origin": "codenvy/ubuntu_jdk8"
  },
  "workspaceConfig": {
    "environments": {
      "default": {
        "machines": {
          "dev-machine": {
            "agents": [
              "org.eclipse.che.terminal",
              "org.eclipse.che.ws-agent",
              "org.eclipse.che.ssh"
            ],
            "servers": {},
            "attributes": {
              "memoryLimitBytes": "2147483648"
            }
          }
        },
        "recipe": {
          "content": "services:\n dev-machine:\n  image: codenvy/ubuntu_jdk8\n",
          "contentType": "application/x-yaml",
          "type": "compose"
        }
      }
    },
    "name": "default",
    "defaultEnv": "default",
    "description": null,
    "commands": []
  }
}